<span class="k">while</span> <span class="ow">not</span> <span class="p">(</span><span class="n">U</span><span class="o">.</span><span class="n">leave</span><span class="p">(</span><span class="n">He</span><span class="p">)</span> <span class="ow">and</span> <span class="n">U</span><span class="o">.</span><span class="n">accept</span><span class="p">(</span><span class="n">Me</span><span class="p">)):</span>  
